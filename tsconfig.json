{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "compilerOptions":{
    "types": ["element-plus/global","node","electron"],
    "baseUrl": "./",
    "paths": {
      "@/*":["src/*"]
    },
    "moduleResolution": "Node",
    "module": "ESNext"
  },
  "include": [
    "src/**/*.vue",
    "src/**/*.ts",
    "*.d.ts",
  ],
  "exclude": ["node_modules"],
}
